{
  "ConnectionStrings": {
    "DefaultConnection": "Server=.;Database=NitecoTest;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "AllowOrigins": "*",
  "Tokens": {
    "Key": "0123456789ABCDEF",
    "Issuer": "http://ktapi.ktcungae.net"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "logs/daily-.txt",
                "rollingInterval": "Day",
                "retainedFileCountLimit": 7,
                "buffered": true
              }
            }
          ]
        }
      }
    ]
  }
}